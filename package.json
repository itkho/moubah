{
    "name": "moubah",
    "productName": "Moubah",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": "src/main.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development electron-forge start",
        "prod": "cross-env NODE_ENV=production electron-forge start",
        "package": "cross-env NODE_ENV=production electron-forge package",
        "make": "cross-env NODE_ENV=production electron-forge make",
        "publish": "cross-env NODE_ENV=production electron-forge publish"
    },
    "keywords": [],
    "author": {
        "name": "Karim EL HAJOUI",
        "email": "karimelhajoui63@gmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "moubah"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.8.0",
        "detect-port": "^1.5.1",
        "electron-squirrel-startup": "^1.0.0",
        "fastq": "^1.14.0",
        "ffmpeg-static": "^5.1.0",
        "ffprobe-static": "^3.1.0",
        "fs-jetpack": "^5.1.0",
        "image-downloader": "^4.3.0",
        "video.js": "^7.20.3",
        "yt-search": "^2.10.3",
        "ytdl-core": "^4.11.2"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.67",
        "@electron-forge/maker-deb": "^6.0.0-beta.67",
        "@electron-forge/maker-rpm": "^6.0.0-beta.67",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
        "@electron-forge/maker-zip": "^6.0.0-beta.67",
        "adm-zip": "^0.5.10",
        "cross-env": "^7.0.3",
        "electron": "^22.0.0",
        "electron-builder": "^22.10.3",
        "electronmon": "^2.0.2",
        "glob": "^8.0.3",
        "is-wsl": "^2.2.0",
        "prettier": "2.8.1"
    },
    "build": {
        "appId": "com.moubah.app"
    }
}
