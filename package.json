{
    "name": "moubah",
    "version": "1.0.0",
    "license": "AGPL-3",
    "main": "main/out/index.js",
    "author": {
        "name": "Karim Bouchez",
        "email": "bouchez.karim@gmail.com"
    },
    "description": "Desktop application to play YouTube videos without background music",
    "repository": {
        "url": "https://github.com/karim-bouchez/moubah"
    },
    "keywords": [],
    "productName": "Moubah",
    "scripts": {
        "i18n": "npm run i18n:extract && npm run i18n:compile",
        "i18n:clean": "lingui extract --clean",
        "i18n:extract": "lingui extract",
        "i18n:compile": "lingui compile --typescript",
        "dev": "cross-env PINO_LOG_LEVEL=debug NODE_ENV=development concurrently \"npm run dev:renderer\" \" npm run dev:main\"",
        "dev:renderer": "vite",
        "dev:main": "npm run build:main && electron .",
        "build": "npm run build:renderer && npm run build:main",
        "build:renderer": "vite build",
        "build:main": "tsc -p main",
        "dist": "npm run build && cross-env USE_HARD_LINKS=false electron-builder --publish=never",
        "pack": "npm run build && electron-builder --dir",
        "clean": "rimraf dist main/out renderer/out",
        "type-check": "tsc",
        "lint": "eslint . --ext js,jsx,ts,tsx",
        "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
    },
    "dependencies": {
        "@aptabase/electron": "^0.2.2",
        "@fortawesome/fontawesome-svg-core": "^6.2.1",
        "@fortawesome/free-brands-svg-icons": "^6.2.1",
        "@fortawesome/free-regular-svg-icons": "^6.2.1",
        "@fortawesome/free-solid-svg-icons": "^6.2.1",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@grpc/grpc-js": "^1.8.2",
        "@headlessui/react": "^1.7.9",
        "@heroicons/react": "^2.0.13",
        "@lingui/react": "^3.17.1",
        "@tailwindcss/line-clamp": "^0.4.2",
        "@types/yt-search": "^2.3.2",
        "compare-versions": "^6.0.0-rc.1",
        "country-flag-icons": "^1.5.5",
        "detect-port": "^1.5.1",
        "dotenv": "^16.0.3",
        "electron-fetch": "^1.9.1",
        "electron-is-dev": "^2.0.0",
        "electron-settings": "^4.0.2",
        "electron-squirrel-startup": "^1.0.0",
        "fastq": "^1.14.0",
        "ffmpeg-static": "^5.1.0",
        "ffprobe-static": "^3.1.0",
        "firebase": "^9.17.1",
        "fs-extra": "^11.1.1",
        "google-protobuf": "^3.21.2",
        "image-downloader": "^4.3.0",
        "os-locale-s": "^1.0.8",
        "pino": "^8.8.0",
        "pino-pretty": "^9.1.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-player": "^2.11.0",
        "react-toastify": "^9.1.1",
        "sanitize-filename": "^1.6.3",
        "tailwind-scrollbar": "^2.1.0",
        "tree-kill": "^1.2.2",
        "uuid": "^9.0.0",
        "video.js": "^7.20.3",
        "vite": "^2.8.6",
        "yt-search": "^2.10.3",
        "ytdl-core": "^4.11.5"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@lingui/cli": "^3.17.1",
        "@lingui/core": "^3.17.1",
        "@lingui/macro": "^3.17.1",
        "@types/google-protobuf": "^3.15.6",
        "@types/node": "^17.0.22",
        "@types/react": "^17.0.53",
        "@types/react-dom": "^17.0.18",
        "@types/uuid": "^9.0.2",
        "@typescript-eslint/eslint-plugin": "5.16.0",
        "@typescript-eslint/parser": "5.16.0",
        "@vitejs/plugin-react": "^1.0.7",
        "autoprefixer": "^10.4.4",
        "babel-plugin-macros": "^3.1.0",
        "concurrently": "^7.0.0",
        "cross-env": "^7.0.3",
        "electron": "^17.1.2",
        "electron-builder": "^24.4.0",
        "eslint": "8.11.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "grpc-tools": "^1.12.3",
        "postcss": "^8.4.12",
        "prettier": "^2.8.1",
        "prettier-plugin-tailwindcss": "^0.2.2",
        "tailwindcss": "^3.0.23",
        "ts-protoc-gen": "^0.15.0",
        "typescript": "^4.6.2"
    },
    "build": {
        "asar": false,
        "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
        "directories": {
            "buildResources": "renderer/assets"
        },
        "mac": {
            "target": "dmg",
            "icon": "renderer/assets/icon-mac.png",
            "files": [
                "main/out",
                "renderer/out",
                "config.json",
                "music-remover"
            ]
        },
        "win": {
            "target": "nsis",
            "icon": "renderer/assets/icon-win.png",
            "files": [
                "main/out",
                "renderer/out",
                "config.json",
                "music-remover/dist"
            ]
        }
    }
}
